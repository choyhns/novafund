<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.bizCheck">

<insert id="insertBizCheck" parameterType="com.exe.fund.dto.BizCheckDTO">
	insert into bizCheck(bNum, userId, pName, bizStartDate,result) values 
	(#{bNum},#{userId},#{pName},#{bizStartDate},#{result})
</insert>

<select id="getReadBizNum" resultType="com.exe.fund.dto.BizCheckDTO" parameterType="String">
	select * from bizCheck 
	where userId = #{userId}
</select>

<update id="updateBizCheck" parameterType="com.exe.fund.dto.BizCheckDTO">
	update bizCheck set bNum=#{bNum}, pName=#{pName}, bizStartDate=#{bizStartDate},
	result=#{result} where userId = #{userId}
</update>

<delete id="deleteBizNum" parameterType="String">
	delete from bizCheck where bNum = #{bNum}
</delete>

<delete id="deleteBizAll" parameterType="int">
	delete from bizCheck where numPro = #{numPro}
</delete>

</mapper>
